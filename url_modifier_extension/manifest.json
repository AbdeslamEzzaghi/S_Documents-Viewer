{
  "manifest_version": 3,
  "name": "Scribd Viewer",
  "version": "1.0",
  "description": "A Chrome extension to modify Scribd URLs",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "400": "icons/icon400.png"
    }
  },
  "icons": {
    "400": "icons/icon400.png"
  }
}

